	/*topnew*/
	.fa-phone {font-size:12px;width: 20px;height: 20px;color:#fdc230;border:1px solid #fdc230;border-radius: 50%;padding:3px 4px;}
	.div-tel{width: 200px;}
	.top-tel{font-size:12px;color: #4d4d4d;letter-spacing: 0.2px;}
	.logo{float:left;display: block;}
	.logo img{text-align: left;margin-left: 0;height: 60px;}
	.menu-logo{padding: 15px 0;position: relative;}

	.top{width: 100%;background-color: #fafafa;line-height: 2.8;height: 36px;}
	.top-right {position: relative;font-size: 12px;letter-spacing: 1px;}
	.top-right > li {
		padding-right: 20px;
		padding-left: 20px;
	}
	.top-right li:last-child {border-right:0;padding-right: 0;} 

	.top-right li a{font-size: 12px;color: #4d4d4d;letter-spacing: 1px;}
	.top-right li a.on{color: #4b92ff;}

	/*帮助中心导航*/
	.menu{margin-top: 14px; float:left;margin-left:50px;}
	.menu li{position: relative;margin-right: 15px;}	
	.top-right li a.one-level{
		display:inline-block;
		color: #fdc230;
		padding-left: 3px;
		padding-right: 3px;
		margin-top:2px;
		border:1px solid transparent;
		border-bottom:0;
		border-top-right-radius:4px;
		border-top-left-radius:4px;	
	}
	.top-right li:hover a.one-level {border-color: #e5e5e5;}
	.top-right .two-level a{font-size:12px;color: #808080;}
	.menu .two-menu a{font-size:14px;color: #808080;}
	.top-right .two-level li:hover a,
	.menu .two-menu li:hover a {color: #fdc230;}
	.top-right .two-level {
		display: none;
		position: absolute; 
		z-index: 9999;
		width:73px;
		top:36px;
		background-color: #fff;
		border:1px solid #e5e5e5; 
		border-top:0;
		border-bottom-right-radius: 4px;
		border-bottom-left-radius: 4px;
	}
	/*首页等导航*/
	.menu .one-menu {
		display: block;
		font-size:16px;
		color: #666;
		padding: 5px 10px; 
		font-weight: 400;
		letter-spacing: 1px;
		border: 1px solid transparent;
		border-bottom: 0;
		border-top-right-radius: 4px;
		border-top-left-radius: 4px;
	}
	.menu li:hover .one-menu {border-color: #e5e5e5}		
	.menu .two-menu {
		display: none;
		position: absolute; 
		z-index: 9999;
		width:107px;
		padding-top: 10px;
		top:28px;
		padding-bottom: 10px;
		letter-spacing: 1px;
		border: 1px solid #e5e5e5;
		border-top: 0;
		border-bottom-right-radius: 4px;
		border-bottom-left-radius: 4px;    
		background-color: #fff;
	}
	.top-right .two-level li,
	.menu .two-menu li{
		text-align: center;
		line-height: 28px;
		height: 28px;
	}
	.menu .two-menu li{margin-right: 0;}
	.menu a.active,.menu a.one-menu:hover{color: #fdc230;border-top-right-radius:4px;border-top-left-radius:4px;}
	.fa-user {
		font-size: 16px;
		padding-right: 4px;
	}
	.one-menu.my-account {
		font-size: 14px;
		border:1px solid #e5e5e5;
		border-bottom-right-radius: 4px;
		border-bottom-left-radius: 4px;
	}
	.divider {height: 16px;width: 1px;background-color: #ccc;position: absolute;top:30%;}



	/*登录注册浮层*/
	.login-box {
		background-color: rgba(0,0,0,0.5);
		position: absolute;
		top:23px;
		left: 67.5%;		
		width: 252px;
		height: 274px;
		border-radius: 6px;
		z-index: 10;
		text-align: center;
	}
	.login-box-content{
		padding: 50px 16px 36px;
		color:#fff;
	}
	.login-box-content > h5{
		font-weight: 500;
		font-size: 20px;
	}
	.login-box-content h1 i { 
		color: #ffa70d; 
		font-style: normal;	
		font-weight: 500;
	}
	.login-box-content h1 > .num {
		font-size: 50px;
	}
	.login-box-content h1 > .per {font-size: 24px;}

	.login-box-desc{
		padding-bottom: 15px;
	}
    /*按钮样式*/
	.btn-default {
		color: #fff;
		background-color: #fdc230;
		border-color: #fdc230;
		border-radius: 4px;
	}
	.btn-default:hover,
	.btn-default:focus,
	.btn-default.focus,
	.btn-default:active,
	.btn-default.active,
	.open > .dropdown-toggle.default {
		color: #fff;
		background-color: #fdc230;
		border-color: #fdc230;
	} 

	.login-box-content > .btn {
		width: 220px;
		height: 44px;
		line-height: 30px;
		font-size: 16px;
	}
	.login-box-content > .text-right {
		color: #fff;
		font-size: 14px;
		padding-top: 20px
	}
	.login-box-content .text-right > a {
		color: #fff;
		text-decoration:underline;
	}

	.bus-des{
		border:1px solid #dcdcdc;
		padding: 15px;
	}
	.bus-des .icon{
		float: left;
		margin-right: 15px;
	}
	.bus-des .icon-color-1{
		color: #1b85ee;
	}
	.icon-list{margin-top: 30px;}
	.data-list{margin-top:30px;}
	.data-list div{text-align: center;}
	.data-list span{font-size: 18px;color: gray; display: inline-block;}
	.data-list strong{font-size:24px;color: red;}
	.data-list div{border-right: 1px solid #dcdcdc;}
	.data-list div div:last-of-type{border-right: none;}
	.data-list .data-border{border:1px solid #dcdcdc;padding:30px;}

	.project h1{width:100%;margin:30px auto 0 auto; text-align: center; position: relative;margin-bottom: 15px; font-size: 24px;}
	.project h1:after{display: block;position: absolute; background-color: #dcdcdc;width: 100%;height: 3px; top:15px; z-index: -10; content: ' ';}
	.project h1 span{background-color: #fff;display: inline-block;padding: 0 15px; z-index: 100}
	.project .project-border{border:1px solid #dcdcdc;padding: 30px 0;margin-bottom: 30px;}
	.project table{width: 100%;}
	.project table,.project table td{border:0;border-collapse: collapse;text-align: center;}
	.project table .p-title{font-weight: 500;font-size: 14px;color: #b2b2b2;width: 8%;}
	.project table .p-num{font-size: 24px;font-weight: 500;padding-top: 24px;width: 8%;}
	.project table .text-red{font-size: 24px;color: red;font-weight: 600;}
	.project table .p-bno{font-size: 18px;}
	.project div.project-border:last-of-type{
		margin-bottom: 0;
	}
	.info-list{
		border:1px solid #dcdcdc;
		padding: 30px 0
	}
	.info-list .info-list-notice,.info-list .info-list-team,.info-list .info-list-top{
		float:left;
	}
	.info-list .info-list-notice{width:30%;}
	.info-list .info-list-notice ul,.info-list .info-list-notice ul li{list-style: none;margin:0;padding:0;}
	.info-list .info-list-notice ul li{padding-left:30px;padding-top:5px; padding-bottom: 5px;}
	.info-list .info-list-notice ul li span{color: gray;}
	.info-list .info-list-team{width:30%;}
	.info-list .info-list-top{width: 40%;}
	.info-list-img{text-align: center;}
	.info-list h5{font-size:16px; text-align: center; font-weight: 500;}
	.info-list>div:nth-child(2){border-right: 1px solid #dcdcdc;border-left:1px solid #dcdcdc;}
	.info-list-top dl{margin-left:30px;}
	.info-list-top dl dd{float:left; padding-right: 15px;}
	.info-list-top dl dd a{display: inline-block;padding-left:8px; padding-right: 8px;padding-top:3px; padding-bottom:3px;text-overflow: none; color: #333;}
	.info-list-top dl dd a.active,.info-list-top dl dd a:hover{background-color:#1b85ee;color:#fff; }
	.info-list-top ul,.info-list-top li{margin:0;padding:0; list-style: none;}
	.info-list-top li{padding-left:30px;width:50%; float:left;line-height: 28px;}
	.foot{background-color: #313131;padding:30px 30px;margin-top:30px; color: #fff;}
	.foot>.help>div{float:left;}
	.foot>.help>div:nth-child(1){width:30%;padding-left:30px;}
	.foot>.help>div:nth-child(2){width:30%;border-left:1px solid #666;border-right: 1px solid #666;padding-left:30px;}
	.foot>.help>div:nth-child(3){width:40%;padding-left:30px;}
	.foot-qrcode dl dd{ width: 35%;float:left; font-size: 14px; line-height: 190%;color: #999;}
	.foot-qrcode dl dd img{width:100px;}
	.foot-help{position: relative;}
	.foot-help:before{content: ' ';display: block;top:0; bottom:0;left:0;width:1px;background-color: #666; position: absolute;}
	.foot-help ul,.foot-help li{margin:0;padding: 0;list-style: none}
	.foot-help ul li{line-height: 24px;}
	.foot a{color: #999;}
	.foot a:hover{color: red;}
	.foot .link-icon img{display: inline-block;margin-right: 10px;}
	.foot .copyright{color: gray;padding-top: 15px;}
	.foot .foot-partner img{width: 120px;}
	.i-loan h1{width: 300px;margin:30px auto 15px auto; text-align: center;position: relative;z-index: 88}
	.i-loan h1:before{position: absolute;content: ' ';display: block;width: 100%;height: 2px;background-color: #dcdcdc; top:15px; z-index: -1}
	.i-loan h1 span{display: inline-block;background-color: #fff;padding: 0 15px;}


	.i-loan{position: relative;padding-top: 30px;}
	.i-loan h1{margin-bottom: 30px;}
	.flow-list:after{content: ' ';display: block;width: 100%;position: absolute;height: 3px; background-color: #f1f1f1;top:181px;z-index: -2}
	.flow-list>div{text-align: center;}
	.flow-list>div>div.flow-box{display: inline-block;width: 120px;height: 120px;border-radius: 50%;background-color: #f1f1f1; text-align: center;}
	.flow-list .flow-box span{display: inline-block;font-size: 40px;color:#1b85ee;margin-top:12px; }
	.flow-list .flow-box strong{display: block;margin-top:0px;font-size: 16px;font-weight: 500;}
	.flow-list .flow-desc{margin-top: 15px;font-size: 13px; line-height: 180%;margin-bottom: 60px;}
	.project-bg{padding: 60px 0;background-color: #f5f3f4;}
	.project-box{border:1px solid #dcdcdc; background-color: #fff;padding: 30px; text-align: center;}
	.project-box h3{text-align: center;border-bottom: 2px solid #dcdcdc;padding-bottom: 10px;}
	.project-box p{min-height: 60px; text-align: center;}
	.project-box button{padding-left: 35px;padding-right: 35px;}
