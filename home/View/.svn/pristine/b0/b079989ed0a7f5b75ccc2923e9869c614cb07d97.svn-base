<block name="css">
	<style>
		.bg {
			background-color: #1b85ee;
			height: 200px;
			overflow:hidden;
			margin-top: 30px;
			margin-bottom: 20px;
		}
		/*与detail相同的样式*/
		.pro-box {
			background-color: #fff;
			cursor: pointer;
			border: 1px solid #e5e5e5;
			text-align: center;
		}
		.pro-type{
			font-size: 18px;
			height: 50px;
			line-height: 50px;
			background-color: #fafafa;
		}
		.pro-active{
			color: #fdc230;
			background-color: #fff;
			border-top: 2px solid #fdc230;
            /*border-left:1px solid #e5e5e5;
            border-right:1px solid #e5e5e5;*/
        }
        .project {
        	background-color: #fff;
        	border: 1px solid #e5e5e5;
        	border-top:0;
        	margin-top:-3px;
        }
        .pro-box .notice {
        	font-size: 14px;
        	color: #ccc;
        	background-color: #fff;
        	height: 50px;
        	line-height: 50px;
        }
        .project .row .project-border {
        	margin-bottom: 0;
        	border:0;
        	border-top:1px solid #e5e5e5;
        	padding:40px 50px;
        }
        .project .project-border table th {text-align: left;}
        .project-border thead tr th {font-size: 16px;color: #333;}
        .project-border tbody tr td a {font-size: 16px;padding:4px 10px;}
        .project-border i {font-style: normal;}
        .project-border i.f34 {
        	font-size: 34px;
        	color: #fdc230;
        }
        .project-border i.per {
        	font-size: 14px;
        	color: #fdc230;
        }        
        .project-border i.f18 {
        	font-size: 18px;
        	color: #666;
        }  
        .project-border i.f14 {
        	font-size: 14px;
        	color: #999;
        } 
        .project-border table tbody td.p-title {
        	font-size: 14px;
        	color: #b2b2b2;
        }
        .project-border td.p-countdown {color: #fbc230;}

        .btn-success {
        	color: #fdc230;
        	background-color: #fff;
        	border-color: #fdc230;
        	border-radius: 4px;
        	width: 112px;
        	height: 32px;
        }
        .btn-success:hover,
        .btn-success:focus,
        .btn-success.focus,
        .btn-success:active,
        .btn-success.active,
        .open > .dropdown-toggle.btn-success {
        	color: #fff;
        	background-color: #ffce55;
        	border-color: #fdc230;
        }     
        .project-border:hover {
        	background-color: #fffefa;
        	border: 1px solid #fdc230;
        }  


    </style>
</block>
<block name="js">
<script>
$("#fundRaise").click(function () {
    $(this).addClass("pro-active");
    $(this).siblings().removeClass("pro-active");
    $(".content1").show();
    $(".content2,.content3").hide();
});
$("#assetPledge").click(function () {
    $(this).addClass("pro-active");
    $(this).siblings().removeClass("pro-active");
    $(".content2").show();
    $(".content1,.content3").hide();
});
$("#receivable").click(function () {
    $(this).addClass("pro-active");
    $(this).siblings().removeClass("pro-active");
    $(".content3").show();
    $(".content1,.content2").hide();
});
</script>
