<block name="css">
    <style>
        .bg {
            background-color: #1b85ee;
            height: 200px;
            overflow:hidden;
            margin-top: 30px;
            margin-bottom: 20px;
        }
        /*与detail相同的样式*/
        .pro-box {
            background-color: #fff;
            cursor: pointer;
            border-left: 1px solid #e5e5e5;
            border-right: 1px solid #e5e5e5;
            margin-top:-3px;
        }
        .pro-type{
            font-size: 18px;
            height: 50px;
            line-height: 50px;
            background-color: #fafafa;
            border-top:1px solid #e5e5e5;
            margin-top:-3px;
        }
        .pro-type .pro-type-title {height: 47px;border-bottom: 1px solid #e5e5e5;}
        .pro-active{
            color: #fdc230;
            background-color: #fff;
            border-top: 2px solid #fdc230;
            border-left:1px solid #e5e5e5;
            border-right:1px solid #e5e5e5;
            border-bottom: 0 !important;
        }
        .content-box > .project {
            background-color: #fff;
            /*border: 1px solid #e5e5e5;*/
            border-top:0;
            margin-top:-3px;
        }
        .pro-box .notice {
            font-size: 14px;
            color: #ccc;
            background-color: #fff;
            height: 50px;
            line-height: 50px;
        }
        .content-box > .project .project-border {
            margin-bottom: 0;
            border:1px solid #e5e5e5;
            padding:40px 50px;
        }
        .content-box > .project .project-border:hover {
            background-color: #fffefa;
            border-color: #fdc230;
        }
        .content-box > .project .project-border table th {text-align: left;}
        .content-box > .project .project-border thead tr th {font-size: 16px;color: #333;}
        .content-box > .project .project-border tbody tr td a {font-size: 16px;padding:4px 10px;}
        .project-border i {font-style: normal;}
        .project-border i.f34 {
            font-size: 34px;
            color: #fdc230;
        }
        .project-border i.per {
            font-size: 14px;
            color: #fdc230;
        }
        .project-border i.f18 {
            font-size: 18px;
            color: #666;
        }
        .project-border i.f14 {
            font-size: 14px;
            color: #999;
        }
        .project-border table tbody td.p-title {
            font-size: 14px;
            color: #b2b2b2;
        }
        .project-border td.p-countdown {color: #fbc230;}

        .btn-success {
            color: #fdc230;
            background-color: #fff;
            border-color: #fdc230;
            border-radius: 4px;
            width: 112px;
            height: 32px;
        }
        .btn-success:hover,
        .btn-success:focus,
        .btn-success.focus,
        .btn-success:active,
        .btn-success.active,
        .open > .dropdown-toggle.btn-success {
            color: #fff;
            background-color: #fdc230 !important;
            border-color: #fdc230 !important;
        }
        .btn-success.disabled,
        .btn-success[disabled],
        fieldset[disabled] .btn-success,
        .btn-success.disabled:hover,
        .btn-success[disabled]:hover,
        fieldset[disabled] .btn-success:hover,
        .btn-success.disabled:focus,
        .btn-success[disabled]:focus,
        fieldset[disabled] .btn-success:focus,
        .btn-success.disabled.focus,
        .btn-success[disabled].focus,
        fieldset[disabled] .btn-success.focus,
        .btn-success.disabled:active,
        .btn-success[disabled]:active,
        fieldset[disabled] .btn-success:active,
        .btn-success.disabled.active,
        .btn-success[disabled].active,
        fieldset[disabled] .btn-success.active {
            background-color: #fff;
            border-color: #b2b2b2;
        }
        a.btn-sellout,
        a.btn-done {
            color:#b2b2b2;
            border-color: #b2b2b2;
        }
        .p-percent {
            color: #b2b2b2;
        }


    </style>
</block>
<block name="js">
	<script>
		$("#fundRaise").click(function () {
			$(this).addClass("pro-active").css("border-left","0");
			$(this).siblings().removeClass("pro-active");
			$(".content1").show();
			$(".content2,.content3").hide();
		});
		$("#assetPledge").click(function () {
			$(this).addClass("pro-active");
			$(this).siblings().removeClass("pro-active");
			$(".content2").show();
			$(".content1,.content3").hide();
		});
		$("#receivable").click(function () {
			$(this).addClass("pro-active").css("border-right","0");
			$(this).siblings().removeClass("pro-active");
			$(".content3").show();
			$(".content1,.content2").hide();
		});
		$(function () {
            $(".one-menu:eq(1)").addClass("active");

            /*根据不同tid来显示不同区块*/
            var tid = getUrlParam("tid");
            console.log(tid);
            if(tid == 1){
                $("#fundRaise").addClass("pro-active");
                $("#fundRaise").siblings().removeClass("pro-active");
                $(".content1").show();
                $(".content2,.content3").hide();
            }else if(tid == 2){
                $("#assetPledge").addClass("pro-active");
                $("#assetPledge").siblings().removeClass("pro-active");
                $(".content2").show();
                $(".content1,.content3").hide();
            }else if(tid == 3){
                $("#receivable").addClass("pro-active");
                $("#receivable").siblings().removeClass("pro-active");
                $(".content3").show();
                $(".content1,.content2").hide();
            }
        });
        //得到URL参数
        function getUrlParam(name)
        {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)"); //构造一个含有目标参数的正则表达式对象
            var r = window.location.search.substr(1).match(reg);  //匹配目标参数
            if (r != null) return unescape(r[2]); return null; //返回参数值
        }

	</script>
