    <script type="text/javascript">
        $('#myCarousel').carousel({
            interval: 3000
        });
        var num = [0,55,63,80,70,68,18,54,34,79];
        for(var i=1;i<=9;i++){
            var ind = $('.ind-'+i).radialIndicator({
                barColor: '#ff0000',
                barWidth: 6,
                initValue: 0,
                roundCorner : true,
                percentage: true,
                radius:25,
            }).data('radialIndicator');
            ind.animate(num[i]);
        }
